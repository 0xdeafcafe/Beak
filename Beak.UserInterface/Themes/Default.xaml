<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
>
	<!-- Could do pink and orange you know -->

	<SolidColorBrush x:Key="BeakAccentBrush" Color="#3d3d7c" />
	<SolidColorBrush x:Key="BeakAccentBrushSecondary" Color="#d45d80" />

	<Color x:Key="BeakAccentBrushColour" A="#ff" R="#3d" G="#3d" B="#7c" />
	<Color x:Key="BeakAccentBrushColourSecondary" A="#ff" R="#d4" G="#5b" B="#80" />
</ResourceDictionary>
