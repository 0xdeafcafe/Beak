<Application
	x:Class="Beak.App.App"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	StartupUri="Windows/ProjectWindow.xaml"
>
	<Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<!-- External resources -->
				<ResourceDictionary Source="pack://application:,,,/Beak.UserInterface;component/Themes/Modes/Dark.xaml" />
				<ResourceDictionary Source="pack://application:,,,/Beak.UserInterface;component/Themes/Default.xaml" />
				<ResourceDictionary Source="pack://application:,,,/Beak.UserInterface;component/Themes/Generic.xaml" />

				<!-- Internal resources -->
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</Application.Resources>
</Application>
